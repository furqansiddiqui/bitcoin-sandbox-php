{
  "name": "comely-io/starter-app",
  "description": "Comely Starter App",
  "minimum-stability": "dev",
  "require": {
    "php-64bit": ">=7.2.0",
    "ext-bcmath": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-openssl": "*",
    "comely-io/app-kernel": "^2019.310",
    "furqansiddiqui/bitcoin-php": "dev-master"
  },
  "config": {
    "vendor-dir": "../vendor"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "App\\Controllers\\": "controllers/",
      "App\\Database\\": "database/"
    },
    "files": [
      "App.php",
      "AppException.php"
    ]
  }
}